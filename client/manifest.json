{
  "manifest_version": 2,

  "name": "iShine",
  "description": "Overlays Shine API data on craigslist searches",
  "version": "1.0",

  "browser_action": {
   "default_icon": "icon.png",
   "default_popup": "popup.html"
  },
  "permissions": [
   "activeTab"
  ],
  "matches": [
    "*.craigslist.org"
  ],
  "js": ["content.js"]
}
